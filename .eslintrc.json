{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"parserOptions": {
		"sourceType": "commonjs"
	},
	"rules": {
		"no-implicit-globals": "off",
		"no-jquery/no-done-fail": "warn",
		"mediawiki/no-unlabeled-buttonwidget": "warn"
	},
	"globals": {
		"ext": "readonly",
		"ve": "readonly"
	}
}
